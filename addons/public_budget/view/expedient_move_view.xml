<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- SEARCH VIEW -->
        <record id="view_public_budget_expedient_move_filter" model="ir.ui.view">
            <field name="name">public_budget.expedient_move.select</field>
            <field name="model">public_budget.expedient_move</field>
            <field name="arch" type="xml">
                <search string="Expedient Move">
                    <field name="date" string="Date"
                            />
                    <field name="user_id" string="User"
                            />
                    <field name="location_id" string="Source Location"
                            />
                    <field name="location_dest_id" string="Destination Location"
                            />
                    <group expand="0" string="Group By">
                    </group>
                </search>
            </field>
        </record>

        <!-- FORMVIEW -->
        <record id="view_public_budget_expedient_move_form" model="ir.ui.view">
            <field name="name">public_budget.expedient_move.form</field>
            <field name="model">public_budget.expedient_move</field>
            <field name="arch" type="xml">
                <form string="Expedient Move">
                    <header>
                    </header>
                    <sheet string="Expedient Move">
                       <div class="oe_right oe_button_box" name="buttons">
                       </div>
                     <group>
                        <field name="date"
                            /> 
                        <field name="user_id"
                            /> 
                        <field name="location_id"
                            /> 
                        <field name="location_dest_id"
                            /> 
                     </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->
        <record id="view_public_budget_expedient_move_tree" model="ir.ui.view">
            <field name="name">public_budget.expedient_move.tree</field>
            <field name="model">public_budget.expedient_move</field>
            <field name="arch" type="xml">
                <tree string="Expedient Move"
                    editable="top"
                    >
                    <field name="date"
                        />
                    <field name="user_id"
                        />
                    <field name="location_id"
                        />
                    <field name="location_dest_id"
                        />
                </tree>
            </field>
        </record>

    </data>
</openerp>
