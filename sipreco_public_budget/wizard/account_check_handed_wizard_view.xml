<openerp>
    <data>

        <record id="account_check_handed_wizard_view" model="ir.ui.view">
            <field name="name">account.check.handed.wizard.form</field>
            <field name="model">account.check.handed.wizard</field>
            <field name="arch" type="xml">
                <form string="Entregar Cheques">
                    <p class="oe_grey">
                        Va a proceder a entregar los cheques seleccionados. Desea proceder?
                    </p>
                    <footer>
                        <button string="Confirmar" name="action_confirm" type="object" default_focus="1" class="oe_highlight"/>
                        or
                        <button string="Cancelar" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="action_account_check_handed_wizard"
            multi="True"
            key2="client_action_multi" name="Entregar Cheques"
            res_model="account.check.handed.wizard" src_model="account.check"
            view_mode="form" target="new" view_type="form" />

    </data>
</openerp>
