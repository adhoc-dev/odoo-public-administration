<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <!-- <data> -->
    <data noupdate="1">

    <!-- Partners for companies -->
        <record id="base.main_partner" model="res.partner">
            <field name='responsability_id' ref='l10n_ar_invoice.res_IVAE'/>
            <field name='vat'>AR30690884905</field>
            <field name='document_type_id' ref="l10n_ar_invoice.dt_CUIT"/>
            <field name='document_number'>30690884905</field>
            <field name="name">Tribunal Municipal de Cuentas</field>
            <field name="street">Laprida 1527</field>
            <field name="city">Rosario</field>
            <field name="country_id" ref="base.ar"/>
            <field name="state_id" ref="l10n_ar_states.S"/>
            <field name="zip">2000</field>
            <field name="phone">480 2614</field>
            <field name="email"></field>
            <field name="website">www.tmc.gov.ar</field>
            <field name="supplier" eval="True"/>
        </record>
    <!-- Partner bank -->
        <record id="sipreco_public_budget.partner_bank_1" model="res.partner.bank">
            <field name="state">bank</field>
            <field name="acc_number">123456</field>
            <field name='bank' ref='l10n_ar_bank.00065'/>
            <field name="bank_name">Banco Municipal De Rosario</field>
            <field name='journal_id' ref='l10n_ar_invoice.res_IVAE'/>
            <field name='company_id' ref='base.main_company'/>
        </record>

    <!-- Companies -->
        <record id="base.main_company" model="res.company">
            <field name="logo" type="base64" file="sipreco_setup_data_tmc/data/logo_TMC.png"/>
            <field name='currency_id' ref='base.ARS'/>
            <field name="use_argentinian_localization" eval="True"/>
        </record>

    </data>
</openerp>
