<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="claim_type_PA" model="public_budget.subsidy.claim.type">
            <field name="name">Primer aviso</field>
            <field name="code">PA</field>
            <field name="sequence" eval="10"/>
        </record>

        <record id="claim_type_PCD" model="public_budget.subsidy.claim.type">
            <field name="name">A presidencia por Cambio de Destino</field>
            <field name="code">PCD</field>
            <field name="sequence" eval="20"/>
        </record>

        <record id="claim_type_PNR" model="public_budget.subsidy.claim.type">
            <field name="name">A presidencia por No Rendición</field>
            <field name="code">PNR</field>
            <field name="sequence" eval="30"/>
        </record>

        <record id="claim_type_PCS" model="public_budget.subsidy.claim.type">
            <field name="name">A presidencia por Cambio de situación (rendición tardía)</field>
            <field name="code">PCS</field>
            <field name="sequence" eval="40"/>
        </record>

        <record id="claim_type_OR" model="public_budget.subsidy.claim.type">
            <field name="name">Observaciones al responsable</field>
            <field name="code">OR</field>
            <field name="sequence" eval="50"/>
        </record>

        <record id="claim_type_UA" model="public_budget.subsidy.claim.type">
            <field name="name">Ultimo aviso</field>
            <field name="code">UA</field>
            <field name="sequence" eval="60"/>
        </record>

        <record id="seq_approval_arrangement" model="ir.sequence">
            <field name="name">Disposición de aprobacion</field>
            <field name="code">approval_arrangement</field>
            <field name="suffix">%(y)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>