#. module: sipreco_subsidy_management
#: model:ir.ui.view,arch_db:sipreco_subsidy_management.view_public_budget_subsidy_search
msgid ""
"<?xml version=\"1.0\"?>\n"
"<search string=\"Subsidios\">\n"
"                <field name=\"name\"/>\n"
"                <field name=\"amount\"/>\n"
"                <field name=\"partner_id\" string=\"Beneficiary\"/>\n"
"                <field name=\"budget_id\"/>\n"
"                <field name=\"destination\"/>\n"
"                <field name=\"expedient_id\" string=\"Expediente Autorizante\"/>\n"
"                <field name=\"request_expedient_id\"/>\n"
"                <field name=\"parliamentary_resolution_date\"/>\n"
"                <field name=\"parliamentary_expedient\"/>\n"
"                <field name=\"accountability_administrative_expedient_id\"/>\n"
"                <filter string=\"Vencida\" domain=\"[('accountability_state', '!=', 'approved') , ('accountability_expiry_date', '!=', False), ('accountability_expiry_date', '&lt;=', time.strftime('%Y-%m-%d'))]\"/>\n"
"                <filter string=\"Próxima a vencer\" domain=\"[('accountability_state', '!=', 'approved'), ('accountability_expiry_date', '!=', False), ('accountability_expiry_date', '&gt;', time.strftime('%Y-%m-%d')), ('accountability_expiry_date', '&lt;=', (context_today() + datetime.timedelta(15)).strftime('%Y-%m-%d'))]\" help=\"Rendición que vencerá en los próximos 15 días\"/>\n"
"                <filter string=\"Entregados no rendidos\" domain=\"[('pendientes_rendicion_amount', '&gt;', 0.0), ('cargo_amount', '&gt;', 0.0)]\"/>\n"
"                <filter string=\"Pendientes de revisión\" domain=\"[('revision_amount', '&gt;', 0.0)]\"/>\n"
"                    <!-- no lo neceistan, pidieron pendiente de revision -->\n"
"                <!-- <filter\n"
"                    string=\"Pendientes aprobación\"\n"
"                    domain=\"[('pendientes_aprobacion_amount', '&gt;', 0.0), ('cargo_amount', '&gt;', 0.0)]\"\n"
"                    /> -->\n"
"                <filter string=\"Aprobados\" domain=\"[('accountability_state', '=', 'approved'), ('cargo_amount', '&gt;', 0.0)]\"/>\n"
"                <group expand=\"1\" string=\"Group By\">\n"
"                    <filter string=\"Estado de la Transacción\" name=\"state\" context=\"{'group_by':'state'}\"/>\n"
"                    <filter string=\"Estado\" name=\"accountability_state\" context=\"{'group_by':'accountability_state'}\"/>\n"
"                    <filter string=\"Fecha\" name=\"date\" context=\"{'group_by':'date'}\"/>\n"
"                </group>\n"
"            </search>\n"
"        "
msgstr ""

